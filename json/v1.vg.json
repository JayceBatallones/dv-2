{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": 450,
  "config": {
    "background": "#e0fbfc"
  },
  "data": {
      "url": "https://raw.githubusercontent.com/JayceBatallones/dv-2/main/data/ne_10m_admin_2_counties.json",
      "format": {
          "type": "topojson", 
          "feature": "counties"
          }
      },
      "transform": [{
          "lookup": "County",
          "from": {
              "data": {
                  "url": "https://raw.githubusercontent.com/JayceBatallones/dv-2/main/data/Electric_Vehicle_Population_Data.csv"
              },
              "key": "county",
              "fields": ["county"]
          }
      }],
      "projection": {
          "type": "albersUsa"
      },
      "mark": "geoshape",
      "encoding": {
          "color": {
              "field": "county",
              "type": "quantitative"
          }
      }
  }