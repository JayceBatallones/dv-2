{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/JayceBatallones/ev-vis/main/src/assets/Electric_Vehicle_Population_Data.csv"},
    "width": 200,
    "height": 200,
    "title": "Electric Vehicle",
    "config": {
      "background": "#e0fbfc"
    },


    "mark": {"type": "arc", "tooltip": true},
    "encoding": {
      "theta": {"aggregate": "count","field":"Electric Vehicle Type", "type": "quantitative", "stack": "normalize"},
      "color": {"field": "Electric Vehicle Type", "type": "nominal"}
    }
  }